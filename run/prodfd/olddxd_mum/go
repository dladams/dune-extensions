SUF=10
if [ -n "$1" ]; then
  SUF=$1
fi
INNAME=detsim_mum$SUF
OUTNAME=dxd_mum$SUF
INDIR=../olddetsim_mum
INFILE=$INDIR/${INNAME}_event.root
NEVT=10
if [ -n "$2" ]; then
  NEVT=$2
fi
LOG=$OUTNAME.log
FCL=dxd.fcl
COM="lar -c $FCL -s $INFILE -o ${OUTNAME}_event.root -T $OUTNAME.root -n $NEVT"
echo $COM >$LOG
$COM 2>&1 | tee -a $LOG

fcldump $FCL 20 > ${FCL}dump
